#include <stdio.h>
#include <stdlib.h>

int main (){
	
	printf ("%d \n", 78787);
	
	
	printf ("%d \n", 78787+3259);
	
	printf ("la suma de %d + %d es = %d\n", 78787 ,3259,78787+3259);
	system("Pause");
	
	
	
	
	
	printf ("%d \n", 5);
	
	
	printf ("%d \n", 5+8);
	
	printf ("la suma de %d + %d es = %d\n", 5 ,8,5+8);
	system("Pause");
	return 0;
	
}
